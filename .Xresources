! Xresource file
URxvt*depth: 12
URxvt*geometry: 80x30
URxvt.borderColor: #7f20ff
URxvt.internalBorder: 4
! -- scrollbar
URxvt.scrollstyle: plain
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWidthBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.iconFile:     /home/andrew/Pictures/icons/Terminal-icon.png
URXvt.tabbed.reopen-on-close:  no

! -- login shell
URxvt.loginShell: false
! -- font
URxvt.font: xft:Terminus:pixelsize=18
URxvt.boldFont: xft:Terminus:pixelsize=18

! -- perl
URxvt.keysym.Mod4-v: perl:clipboard:paste
urxvt.perl-ext-common: default,matcher,clipboard
URxvt.url-select.launcher:  google-chrome
URxvt.url-select.underline: true
urxvt*saveLines: 1200000
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.iso14755: False

!URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
!URxvt.keysym.M-u:           perl:url-select:select_next
!URxvt.keysym.M-Escape:      perl:keyboard-select:activate
!URxvt.keysym.M-s:           perl:keyboard-select:search

! special
*.foreground:   #7f20ff
*.background:   #020005
*.cursorColor:  #7f20ff

! black
*.color0:       #1d1724
*.color8:       #b4d657

! red
*.color1:       #26202f
*.color9:       #de92b4

! green
*.color2:       #383044
*.color10:      #ee96be

! yellow
*.color3:       #514661
*.color11:      #6ecdb0

! blue
*.color4:       #716186
*.color12:      #bda2e1

! magenta
*.color5:       #9386a5
*.color13:      #d7c8ee

! cyan
*.color6:       #b9b0c5
*.color14:      #72d3f4

! white
*.color7:       #e4e1e9
*.color15:      #7bb3c9
